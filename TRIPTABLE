
CREATE TABLE TP_MUSTEAT(

	TP_ME_SEQ NUMBER(8) PRIMARY KEY,
	TP_ME_PLACE VARCHAR2(50) NOT NULL,
	TP_ME_ID VARCHAR2(50) NOT NULL, 
	TP_ME_NAME VARCHAR2(50) NOT NULL,
	
	TP_ME_TITLE VARCHAR2(200) NOT NULL,
	TP_ME_CONTENT VARCHAR2(4000) NOT NULL,
	
	TP_ME_FILENAME VARCHAR2(50) NOT NULL,
	TP_ME_NEWFILENAME VARCHAR2(50) NOT NULL,
	TP_ME_READCOUNT NUMBER(8) NOT NULL,
	TP_ME_DOWNCOUNT NUMBER(8) NOT NULL,
	TP_ME_REGDATE DATE NOT NULL
);

CREATE SEQUENCE TP_ME_SEQ_MUSTEAT
START WITH 1
INCREMENT BY 1;

SELECT * FROM TP_MUSTEAT;

INSERT INTO TP_MUSTEAT(TP_ME_SEQ, TP_ME_PLACE)


" SELECT TP_ME_SEQ,"
				+ " TP_ME_PLACE,"
				+ " TP_ME_ID,"
				+ " TP_ME_NAME,"
				+ " TP_ME_TITLE,"
				+ " TP_ME_CONTENT,"
				+ " TP_ME_FILENAME,"
				+ " TP_ME_NEWFILENAME,"
				+ " TP_ME_READCOUNT,"
				+ " TP_ME_DOWNCOUNT,"
				+ " TP_ME_REGDATE"
				+ " FROM TP_MUSTEAT"
				+ " ORDER BY TP_ME_SEQ DESC"

